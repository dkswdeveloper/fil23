<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>web1</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>hello2</servlet-name>
		<servlet-class>com.dk.servlet.Hello</servlet-class>
		<init-param>
			<param-name>key</param-name>
			<param-value>mykey2</param-value>
		</init-param>
		<load-on-startup>50</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>hello2</servlet-name>
		<url-pattern>/hello2/*</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>dburl</param-name>
		<param-value>jdbc:mysql://localhost:3306/fil23</param-value>
	</context-param>
	<context-param>
		<param-name>dbpassword</param-name>
		<param-value>password</param-value>
	</context-param>
	<context-param>
		<param-name>dbuser</param-name>
		<param-value>root</param-value>
	</context-param>
<!-- 	<listener>
		<listener-class>com.dk.config.LibraryContextListener</listener-class>
	</listener>
 -->
</web-app>